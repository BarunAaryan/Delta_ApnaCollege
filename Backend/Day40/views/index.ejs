<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mini whatsapp app</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <h3>
        All Chats!
    </h3>
    <% for(let chat of chats){ %>
        <div class="chat">
            <p><i>From: <%= chat.from %></i></p>
            <div class="msg"><p> <%= chat.msg %></p> </div>
            <p><i>Received by: <%= chat.to %></i></p>
            <hr>
        </div>
        <br>
        <br>
        <%  } %>
       

</body>
</html>
